{
  "name": "rosettanet",
  "version": "1.0.0",
  "description": "<div align=\"center\">\r   <img src=\"docs/logos/rosettanet.png\" alt=\"rosettanet\" height=\"300\"/>",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start:dev": "npx nodemon",
    "lint": "eslint . --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test:utils": "jest --watch tests/utils",
    "test:converters": "jest --watch tests/utils/converters",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "docker start devnet-container && npx nodemon --devnet --enable-logs --logging-type console --sniffer && jest --watch e2e/tests",
    "start:file-logging": "npx nodemon --enable-logs --logging-type file --logging-file logs.md",
    "start:console-logging": "npx nodemon --enable-logs --logging-type console",
    "start:dev:console-sniff": "npx nodemon --enable-logs --logging-type console --sniffer",
    "start:dev:file-sniff": "npx nodemon --enable-logs --logging-type file --logging-file sniffs.log --sniffer",
    "start:dev:console-error-sniffer": "npx nodemon --enable-logs --logging-type console --sniffer --min-log 1",
    "build:devnet": "docker build -t devnet . && docker run -d -p 127.0.0.1:5050:5050 --name devnet-container devnet",
    "start:devnet": "docker start devnet-container",
    "stop:devnet": "docker stop devnet-container && docker rm devnet-container",
    "start:dev+devnet:console-sniff": "docker start devnet-container && npx nodemon --devnet --enable-logs --logging-type console --sniffer"
  },
  "keywords": [],
  "author": "Digine-Labs",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bignumber.js": "^9.1.2",
    "ethers": "^6.13.1",
    "express": "^4.18.2",
    "js-sha3": "^0.9.3",
    "starknet": "^6.11.0"
  }
}
